
{% extends "::layout.html.twig" %}
 
{% block docedit_body %}
 <h1>Liste des acteurs</h1>

<table>
{% for a in acteurs %}
	
	<tr>
		<td>{{ a.getCivAct() }}</td>
		<td>{{ a.getNomAct() }}</td>
		
	</tr>
	
{% else %}

	<tr><td>Aucun acteur n'a été trouvé.</td></tr>
	
{% endfor %}
</table>
 
{% endblock %}